def merge_dicts(dict1, dict2):
    # Create a new dictionary to store the merged result
    merged_dict = {}

    # Add key-value pairs from the first dictionary
    for key, value in dict1.items():
        merged_dict[key] = value

    # Add key-value pairs from the second dictionary
    for key, value in dict2.items():
        merged_dict[key] = value

    return merged_dict

def main():
    try:
        # Take input for the first dictionary
        dict1_str = input()
        dict1 = eval(dict1_str)

        # Take input for the second dictionary
        dict2_str = input()
        dict2 = eval(dict2_str)

        # Merge dictionaries and print the result
        merged = merge_dicts(dict1, dict2)
        print(merged)
    except SyntaxError:
        print("Invalid input format. Please enter dictionaries in the correct format.")

if __name__ == "__main__":
    main()
